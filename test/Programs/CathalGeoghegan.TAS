program Test {
    const
        a := 67;
        b := true;
    
    void Main() {
        int ik;
        string testString;
        ik := a>=67 ? 1+10 : 0+10*8;
        write ik;
        testString := false ? "true" : "false";
        write testString;
        testString := true ? "true" : "false";
        write testString;
        int i;
        int j;
        
        // Nested for loop
        for(i := 0; i := i+1; i < 10)
        {
            i := i+1;
            write "i", i;
            for(j := 0; j := j+1; j < 10)
            {
